<?xml version='1.0' encoding='utf-8'?>
<widget id="com.midwestauction.m" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Midwestauction</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="matt@thiessen.us" href="http://matt.thiessen.us/">
        Matt Thiessen
    </author>
    <plugin name="cordova-plugin-statusbar" source="npm" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="1.4.0" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <content src="index.html" />
</widget>
